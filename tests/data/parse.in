parse -s ^no-groups:$ '\d\w?'
parse -s ^json-dict:$ --json '(?P<one>\d)(?P<two>\w)?'
parse -s ^json-list:$ --json '(\d)(\w)?'
parse -s ^json-no-groups:$ --json '\d\w?'
