--section '(\S+):$'
match-span -s ^simple$ --start 2 --end 4
match-span -s ^simple-invert$ --start 2 --end 4 -v
match-span -s ^no-end$ --start 2
match-span -s ^no-end-invert$ --start 2 -v
match-span -s ^no-start$ --end 4
match-span -s ^no-start-invert$ --end 4 -v
match-span -s ^repl$ --start 2 --end 4 --repl ...
match-span -s ^repl-invert$ --start 2 --end 4 --repl ... -v
match-span -s ^repl-edges$ --start 1 --end 5 --repl ...
match-span -s ^repl-invert-one$ --start 2 --end 3 --repl ... -v
match-span -s ^repl-no-end$ --start 2  --repl ...
match-span -s ^repl-two-ends$ --start 3 --end [45] --repl ...
match-span -s ^repl-invert-two-starts$ --start [23] --end 5 --repl ... -v
match-span -s ^repl-capture$ --start '(2)' --end 4 --repl '>\\<'
match-span -s ^repl-invert-capture$ --start '(2)' --end 4 --repl '>\1<' -v

